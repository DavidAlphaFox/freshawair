package "ezpostgresql" {
  opam-version: "2.0"
  version: "~dev"
  maintainer: "Bobby Priambodo <bobby.priambodo@gmail.com>"
  authors: "Bobby Priambodo <bobby.priambodo@gmail.com>"
  license: "MIT"
  homepage: "https://github.com/bobbypriambodo/ezpostgresql"
  doc: "https://bobbypriambodo.github.io/ezpostgresql/"
  bug-reports: "https://github.com/bobbypriambodo/ezpostgresql/issues"
  depends: [
    "ocaml"
    "dune" {build}
    "lwt" {>= "3.0"}
    "postgresql" {>= "4.0"}
    "lwt_ppx" {with-test}
    "alcotest" {with-test & >= "0.8"}
    "alcotest-lwt" {with-test & >= "0.8"}
    "result"
  ]
  build: [
    ["dune" "subst"] {pinned}
    ["dune" "build" "-p" name "-j" jobs]
  ]
  dev-repo: "git+https://github.com/bobbypriambodo/ezpostgresql"
  url {
    src: "git+https://github.com/ManasJayanth/ezpostgresql#5664b90"
  }
}
package "lsp" {
  opam-version: "2.0"
  version: "~dev"
  synopsis: "LSP protocol implementation in OCaml"
  description: """
Implementation of the LSP protocol in OCaml. It is designed to be as portable as
possible and does not make any assumptions about IO."""
  maintainer: "Rudi Grinberg <me@rgrinerg.com>"
  authors: [
    "Andrey Popp <8mayday@gmail.com>"
    "Rusty Key <iam@stfoo.ru>"
    "Louis Roché <louis@louisroche.net>"
    "Oleksiy Golovko <alexei.golovko@gmail.com>"
    "Rudi Grinberg <me@rgrinberg.com>"
    "Sacha Ayoun <sachaayoun@gmail.com>"
    "cannorin <cannorin@gmail.com>"
  ]
  license: "ISC"
  homepage: "https://github.com/ocaml/ocaml-lsp"
  bug-reports: "https://github.com/ocaml/ocaml-lsp/issues"
  depends: [
    "stdlib-shims"
    "yojson"
    "ppx_yojson_conv_lib"
    "ocaml-syntax-shims"
    "cppo"
    "uutf"
    "csexp"
    "menhir" {with-test}
    "cinaps" {with-test}
    "ppx_expect" {with-test & >= "v0.14.0"}
    "ocaml" {>= "4.06"}
    "dune" {>= "2.0"}
  ]
  build: [
    ["dune" "subst"] {pinned}
    ["dune" "build" "-p" name "-j" jobs "@install" "@doc" {with-doc}]
  ]
  dev-repo: "git+https://github.com/ocaml/ocaml-lsp.git"
  url {
    src: "git+https://github.com/ocaml/ocaml-lsp"
  }
}
package "ocaml-lsp-server" {
  opam-version: "2.0"
  version: "~dev"
  synopsis: "LSP Server for OCaml"
  description: "An LSP server for OCaml."
